<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:title="@string/pref_open_source_libs" >
        <Preference android:title="@string/prefs_butter_knife" >
            <intent android:action="android.intent.action.VIEW"
                    android:data="http://jakewharton.github.io/butterknife/" />
        </Preference>
        <Preference android:title="@string/prefs_leak_canary" >
            <intent android:action="android.intent.action.VIEW"
                    android:data="https://github.com/square/leakcanary" />
        </Preference>
        <Preference android:title="@string/prefs_rxandroid" >
            <intent android:action="android.intent.action.VIEW"
                    android:data="https://github.com/ReactiveX/RxAndroid" />
        </Preference>
    </PreferenceCategory>
</PreferenceScreen>